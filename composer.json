{
    "name": "pest-register/php-accounting-common",
    "description": "A common gateway package for all accounting softwares",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dylan Aird",
            "email": "dylan@pestregister.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "psr/http-message": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.0",
        "symfony/http-foundation": "^2.1||^3||^4"
    },
    "require-dev": {
        "php-http/mock-client": "^1.0",
        "php-http/message": "^1.0",
        "guzzlehttp/psr7": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PHPAccounting\\Common\\": "src/Common"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
